# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE=products_queue
RABBITMQ_RETRY_COUNT=3
RABBITMQ_RETRY_DELAY=5

# Ivanti API Configuration
IVANTI_API_URL=https://ivanti-api.example.com
IVANTI_API_KEY=your_api_key_here
IVANTI_CLIENT_ID=your_client_id_here
IVANTI_CLIENT_SECRET=your_client_secret_here
IVANTI_TOKEN_URL=https://ivanti-api.example.com/oauth/token
IVANTI_REQUEST_TIMEOUT=30
IVANTI_MAX_RETRIES=3
IVANTI_RETRY_DELAY=5

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=logs

# Scheduler Configuration
SCHEDULE_TIME=02:00
TIMEZONE=UTC

# Application Configuration
ENVIRONMENT=development
DEBUG=false